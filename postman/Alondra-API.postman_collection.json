			]
		},
		{
			"name": "Roles",
			"item": [
				{ "name": "Listar todos", "request": { "method": "GET", "url": "{{baseUrl}}/api/roles", "description": "Obtiene todos los roles (tb_rol)." } },
				{
					"name": "Crear Rol",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"body": { "mode": "raw", "raw": "{\n  \"idRol\": 0,\n  \"tipo\": \"Admin\"\n}" },
						"url": "{{baseUrl}}/api/roles",
						"description": "Crea un rol. Ej. Admin, CLIENTE. Usuarios usan idRol (ManyToOne)."
