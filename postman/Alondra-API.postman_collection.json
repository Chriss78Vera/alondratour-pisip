{
	"info": {
		"_postman_id": "alondra-api-collection-2025",
		"name": "Alondra Backend API",
		"description": "Colección con todas las APIs: creaciones (POST) y consultas (GET) del backend Alondra. Orden sugerido de creación: 1-Agencias, 2-Usuarios, 3-Hoteles, 4-Vuelos, 5-PaquetesDetalles, 6-Paquetes, 7-Reservas, 8-Pasajeros, 9-ServiciosExtra.",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"variable": [
		{ "key": "baseUrl", "value": "http://localhost:8080" }
	],
	"item": [
		{
			"name": "Agencias",
			"item": [
				{
					"name": "Crear Agencia",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"idAgencia\": 0,\n  \"nombre\": \"Agencia Viajes XYZ\",\n  \"telefono\": \"+57 300 123 4567\"\n}"
						},
						"url": "{{baseUrl}}/api/agencias",
						"description": "Crea una nueva agencia."
					}
				},
				{ "name": "Listar todas", "request": { "method": "GET", "url": "{{baseUrl}}/api/agencias" } },
				{ "name": "Buscar por Id", "request": { "method": "GET", "url": "{{baseUrl}}/api/agencias/buscarPorId?id=1" } },
				{ "name": "Buscar por Nombre", "request": { "method": "GET", "url": "{{baseUrl}}/api/agencias/buscarPorNombre?nombre=Agencia Viajes XYZ" } }
			]
		},
