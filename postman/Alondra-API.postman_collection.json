						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"idPaquetesDetalles\": 0,\n  \"fechaInicio\": \"2025-02-01\",\n  \"fechaFin\": \"2025-02-07\",\n  \"precioNeto\": 1500000\n}"
