						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"idHotelReserva\": 0,\n  \"idReserva\": 1,\n  \"idHotel\": 1,\n  \"fechaCheckin\": \"2025-02-01\",\n  \"fechaCheckout\": \"2025-02-05\",\n  \"fechaExtraCheckin\": null,\n  \"fechaExtraCheckout\": null\n}"
						},
