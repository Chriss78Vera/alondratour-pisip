				{ "name": "Buscar por Id Hotel", "request": { "method": "GET", "url": "{{baseUrl}}/api/hoteles-reservas/buscarPorIdHotel?idHotel=1", "description": "Devuelve todas las reservas-hotel para un hotel." } }
			]
		},
