				{
					"name": "Desactivar Reserva (estado a false)",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"idReserva\": 1,\n  \"idUsuario\": 1,\n  \"idVuelo\": 1,\n  \"idPaquete\": 1,\n  \"idAgencia\": 1,\n  \"fechaReserva\": \"2025-01-27\",\n  \"costoTotal\": 2500000,\n  \"estado\": false\n}"
