				{
					"name": "Crear Ciudad",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"body": { "mode": "raw", "raw": "{\n  \"idCiudad\": 0,\n  \"nombre\": \"Bogotá\",\n  \"idPais\": 1\n}" },
						"url": "{{baseUrl}}/api/ciudades",
						"description": "Crea una ciudad. idPais debe existir."
