					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"body": { "mode": "raw", "raw": "{\n  \"idPais\": 0,\n  \"nombre\": \"Colombia\"\n}" },
						"url": "{{baseUrl}}/api/paises",
						"description": "Crea un nuevo país."
					}
				},
				{ "name": "Buscar por Id", "request": { "method": "GET", "url": "{{baseUrl}}/api/paises/buscarPorId?id=1" } }
			]
		},
		{
			"name": "Ciudades",
