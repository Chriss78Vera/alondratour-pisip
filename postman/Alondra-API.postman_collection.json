						"body": {
							"mode": "raw",
							"raw": "{\n  \"idHotelReserva\": 1,\n  \"idReserva\": 1,\n  \"idHotel\": 1,\n  \"fechaCheckin\": \"2025-02-01\",\n  \"fechaCheckout\": \"2025-02-05\",\n  \"fechaExtraCheckin\": \"2025-02-06\",\n  \"fechaExtraCheckout\": \"2025-02-07\"\n}"
						},
