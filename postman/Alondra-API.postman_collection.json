		{
			"name": "Paquetes",
			"item": [
				{
					"name": "Crear Paquete",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"idPaquete\": 0,\n  \"idPaquetesDetalles\": 1,\n  \"nombre\": \"Paquete Caribe\",\n  \"descripcion\": \"Viaje todo incluido a Cartagena con hotel y vuelos\",\n  \"pais\": \"Colombia\",\n  \"ciudad\": \"Cartagena\"\n}"
						},
						"url": "{{baseUrl}}/api/paquetes",
						"description": "Crea un paquete. idPaquetesDetalles debe existir (crear Paquetes Detalles antes)."
					}
				},
				{ "name": "Listar todos", "request": { "method": "GET", "url": "{{baseUrl}}/api/paquetes" } },
				{ "name": "Buscar por Id", "request": { "method": "GET", "url": "{{baseUrl}}/api/paquetes/buscarPorId?id=1" } },
				{ "name": "Buscar por Pais", "request": { "method": "GET", "url": "{{baseUrl}}/api/paquetes/buscarPorPais?pais=Colombia" } },
				{ "name": "Buscar por Ciudad", "request": { "method": "GET", "url": "{{baseUrl}}/api/paquetes/buscarPorCiudad?ciudad=Cartagena" } },
				{ "name": "Buscar por Pais y Ciudad", "request": { "method": "GET", "url": "{{baseUrl}}/api/paquetes/buscarPorPaisYCiudad?pais=Colombia&ciudad=Cartagena", "description": "Trae todos los paquetes que pertenecen a esa combinacion de pais y ciudad." } },
				{ "name": "Buscar por Id Paquetes Detalles", "request": { "method": "GET", "url": "{{baseUrl}}/api/paquetes/buscarPorIdPaquetesDetalles?idPaquetesDetalles=1" } },
				{ "name": "Paises y Ciudades Distintos", "request": { "method": "GET", "url": "{{baseUrl}}/api/paquetes/paisesYCiudadesDistintos", "description": "Devuelve listas sin repetir de paises y ciudades de todos los paquetes." } },
				{ "name": "Detalles con Hoteles por Id Paquete", "request": { "method": "GET", "url": "{{baseUrl}}/api/paquetes/detallesConHoteles?idPaquete=1", "description": "Devuelve idPaquetesDetalles y la lista de hoteles del paquete." } },
				{ "name": "Detalles con Hoteles por Id Paquete e Id Detalle", "request": { "method": "GET", "url": "{{baseUrl}}/api/paquetes/detallesConHotelesPorPaqueteYDetalle?idPaquete=1&idPaquetesDetalles=1", "description": "Recibe idPaquete e idPaquetesDetalles. Valida que ese detalle pertenezca al paquete y devuelve idPaquetesDetalles y sus hoteles." } }
			]
		},
