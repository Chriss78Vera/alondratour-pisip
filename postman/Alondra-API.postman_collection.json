				{ "name": "Buscar por Tipo", "request": { "method": "GET", "url": "{{baseUrl}}/api/roles/buscarPorTipo?tipo=Admin" } }
			]
		},
		{
			"name": "Usuarios",
			"item": [
				{
					"name": "Crear Usuario",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"idUsuario\": 0,\n  \"nombre\": \"Juan Pérez\",\n  \"cedula\": \"1234567890\",\n  \"correo\": \"juan@example.com\",\n  \"idRol\": 1,\n  \"password\": \"miPassword123\",\n  \"estado\": true\n}"
						},
						"url": "{{baseUrl}}/api/usuarios",
						"description": "Crea un nuevo usuario. La contraseña se guarda encriptada (BCrypt) en BD. estado por defecto true."
					}
				},
				{
					"name": "Actualizar Usuario",
					"request": {
						"method": "POST",
						"header": [{ "key": "Content-Type", "value": "application/json" }],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"idUsuario\": 1,\n  \"nombre\": \"Juan Pérez Actualizado\",\n  \"cedula\": \"1234567890\",\n  \"correo\": \"juan.nuevo@example.com\",\n  \"idRol\": 1,\n  \"password\": \"nuevaPassword456\",\n  \"estado\": true\n}"
						},
						"url": "{{baseUrl}}/api/usuarios/actualizar",
						"description": "Actualiza datos del usuario y/o contraseña. Body: UsuariosRequestDto. Si password viene vacío se mantiene la actual. estado modificable (por defecto true)."
					}
				},
				{ "name": "Listar todos", "request": { "method": "GET", "url": "{{baseUrl}}/api/usuarios" } },
				{ "name": "Buscar por Id", "request": { "method": "GET", "url": "{{baseUrl}}/api/usuarios/buscarPorId?id=1" } },
				{ "name": "Buscar por Cedula", "request": { "method": "GET", "url": "{{baseUrl}}/api/usuarios/buscarPorCedula?cedula=1234567890" } },
				{ "name": "Buscar por Correo", "request": { "method": "GET", "url": "{{baseUrl}}/api/usuarios/buscarPorCorreo?correo=juan@example.com" } },
				{ "name": "Buscar por Rol", "request": { "method": "GET", "url": "{{baseUrl}}/api/usuarios/buscarPorRol?rol=CLIENTE" } }
			]
		},
		{
			"name": "Hoteles",
			"item": [
